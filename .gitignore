# Ignore directories generated by Composer
drush/contrib/
vendor/
web/core/
web/modules/contrib/
web/themes/contrib/
web/profiles/contrib/
web/libraries/

# Ignore Drupal's file directories
web/sites/*/files/css
web/sites/*/files/js
web/sites/*/files/php
web/sites/*/files/styles
web/sites/default/files/tmgmt_file/*

# (often useful too)
web/sites/*/private/
web/sites/simpletest

# Ignore sensitive information
web/sites/*/settings.php
web/sites/*/settings.local.php
web/sites/*/services.local.yml

# Ignore .env files as they are personal
/.env
/.editorconfig
/.gitattributes
/.idea/
.vscode/
